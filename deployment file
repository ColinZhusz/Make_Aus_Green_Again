sudo apt update
sudo apt upgrade -y 
sudo apt-get install python3-venv
python3 -m venv env
source env/bin/activate
pip3 install django
git clone https://github.com/ColinZhusz/Make_Aus_Green_Again.git
cd Make_Aus_Green_Again
pip3 install gunicorn
sudo apt-get install -y nginx
gunicorn --bind 0.0.0.0:8000 Make_AUS_Green_Again.wsgi:application
sudo apt-get install -y supervisor
cd /etc/supervisor/conf.d/
sudo touch gunicorn.conf
sudo nano gunicorn.conf (copy gunicorn configuration 在azure配置将ubuntu改为Kevin)
sudo mkdir /var/log/gunicorn
sudo supervisorctl reread
sudo supervisorctl update
sudo supervisorctl status
cd
cd /etc/nginx/sites-available/
sudo touch django.conf
sudo nano django.conf (copy and paste django.conf)
sudo nginx -t
sudo ln django.conf /etc/nginx/sites-enabled/
sudo service nginx restart
(copy past /static in django.conf)
sudo systemctl reload nginx

